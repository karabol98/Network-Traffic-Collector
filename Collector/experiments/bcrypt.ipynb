    "import bcrypt\n",
    "\n",
    "# Password from user\n",
    "password_attempt = \"Adm1n@dm1n\"\n",
    "\n",
    "hash2 = bcrypt.hashpw(password_attempt.encode('utf-8'), bcrypt.gensalt())\n",
    "\n",
    "# Verify\n",
    "if bcrypt.checkpw(password_attempt.encode('utf-8'), hash2):\n",
    "    print(\"✅ Password is correct\")\n",
    "else:\n",
    "    print(\"❌ Invalid password\")"
   ]
  }
